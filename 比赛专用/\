#include <iostream>
#include <stack>
using namespace std;
int main()
{
	int n;
	while(cin>>n)
	{
		unsigned long his[n+5];
		for (int i=1;i<=n;++i)
			cin>>his[i];
		unsigned long stk[n+5];
		int top;
		top=0;
		int maxx=0;
		unsigned long hiss[n+5];
		for(int i =1;i<=n;++i)
		{
			while(top && his[stk[top]]<his[i])
				top--;
			if(!top)
				hiss[i]=0;
			else
				hiss[i]=top;
			stk[++top]=i;
		}
		for (int i=0;i<=n;++i)
			cout<<his[i]<<' ';

	}
}
